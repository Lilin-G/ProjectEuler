      "cell_type": "code",
      "source": [
        "n = 600851475143\n",
        "i = 2\n",
        "while i * i <n:\n",
        "    while n % i == 0:\n",
        "        n = n / i\n",
        "    i = i + 1\n",
        "\n",
        "print (n)"
